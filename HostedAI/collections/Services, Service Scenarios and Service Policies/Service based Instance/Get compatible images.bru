meta {
  name: Get compatible images 
  type: http
  seq: 5
}

get {
  url: {{url}}/service/i/compatible-images?service_id={{service_id}}&cluster_id={{cluster_id}}&team_id={{team_id}}
  body: json
  auth: bearer
}

params:query {
  service_id: {{service_id}}
  cluster_id: {{cluster_id}}
  team_id: {{team_id}}
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "name" : "cPanel",
