meta {
  name: get max vGPUs for min pod requirement
  type: http
  seq: 1
}

get {
  url: http://localhost:8000/max_deployable_vgpus/{{pool_name}}?min_ram_mb=1&min_storage_mb=1&min_cpu_cores=1
  body: none
  auth: inherit
}

params:query {
  min_ram_mb: 1
  min_storage_mb: 1
  min_cpu_cores: 1
  ~node_requirements: node1
  ~node_requirements: hai
}

settings {
  encodeUrl: true
}
