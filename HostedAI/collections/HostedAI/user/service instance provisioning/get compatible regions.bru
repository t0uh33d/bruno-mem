meta {
  name: get compatible regions
  type: http
  seq: 5
}

get {
  url: {{url}}/service/i/compatible-regions?service_id={{service_id}}&team_id={{team_id}}
  body: none
  auth: bearer
}

params:query {
  service_id: {{service_id}}
  team_id: {{team_id}}
}

auth:bearer {
  token: {{token}}
}

settings {
  encodeUrl: true
}
